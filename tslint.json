{
  "rulesDirectory": ["./node_modules/tslint-immutable/rules"],
  "rules": {
    "variable-name": [true, "ban-keywords", "check-format", "allow-snake-case", "allow-pascal-case"],
    "adjacent-overload-signatures": true,
    "await-promise": [true, "Bluebird"],
    "ban-comma-operator": true,
    "curly": true,
    "cyclomatic-complexity": [true, 100],
    "deprecation": true,
    "encoding": true,
    "label-position": true,
    "max-file-line-count": [true, 20000],
    "object-literal-shorthand": true,
    "one-variable-per-declaration": true,
    "only-arrow-functions": [true, "allow-declarations"],
    "ordered-imports": true,
    "prefer-conditional-expression": true,
    "prefer-const": false,
    "prefer-for-of": false,
    "prefer-object-spread": true,
    "promise-function-async": true,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "radix": true,
    "readonly-array": false,
    "readonly-keyword": false,
    "semicolon": [true, "always"],
    "strict-type-predicates": true,
    "triple-equals": true,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration",
      "member-variable-declaration"
    ],
    "use-isnan": true,

    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-console": false,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-switch-case": true,
    "no-duplicate-variable": true,
    "no-empty-interface": true,
    "no-floating-promises": true,
    "no-for-in-array": true,
    "no-import-side-effect": true,
    "no-inferrable-types": true,
    "no-inferred-empty-object-type": true,
    "no-internal-module": true,
    "no-invalid-this": false,
    "no-let": false,
    "no-mixed-interface": true,
    "no-namespace": true,
    "no-non-null-assertion": true,
    "no-object-literal-type-assertion": true,
    "no-reference": true,
    "no-require-imports": true,
    "no-shadowed-variable": true,
    "no-sparse-arrays": true,
    "no-string-literal": false,
    "no-string-throw": true,
    "no-this": false,
    "no-trailing-whitespace": true,
    "no-unsafe-finally": true,
    "no-unused-variable": true,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "no-var-requires": true,

    "indent": false,
    "no-consecutive-blank-lines": false,
    "object-curly-spacing": false,
    "space-before-function-paren": false,
    "ter-indent": false,
    "trailing-comma": false,
    "typedef-whitespace": false,
    "whitespace": false
  }
}
